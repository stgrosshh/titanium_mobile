/**
 * @class <%- cls.name %>
 * <%- cls.summary %>
<%#
   description, osver, examples are all appended to become part of summary before
   we move on to actual tags
-%>
<% if (cls.description) { -%>
 * <%- cls.description %>
<% } -%>
<% if (cls.osver) { -%>
 * <%- cls.osver %>
<% } -%>
<% if (cls.examples) { -%>
 * <%- cls.examples %>
<% } -%>
<% for (platform in cls.since) { -%>
 * @platform <%- platform %> <%- cls.since[platform] %>
<% } -%>
<% if (cls.subtype == 'pseudo') { -%>
 * @pseudo
<% } -%>
<% if ('extends' in cls) { -%>
 * @extends <%- cls.extends %>
<% } -%>
<% if (cls.editurl) { -%>
 * @editurl <%- cls.editurl %>
<% } -%>
<% if (cls.deprecated) { -%>
 * <%- cls.deprecated %>
<% } -%>
 */

<% cls.events.forEach(function (event) { -%>
<%- include('event', { event: event }); %>
<% }); -%>
<% cls.methods.forEach(function (method) { -%>
<%- include('method', { method: method }); %>
<% }); -%>
<% cls.properties.forEach(function (property) { -%>
<%- include('property', { property: property }); %>
<% }); -%>
