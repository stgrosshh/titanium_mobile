/**
 * @event <%- event.name %>
 * <%- event.summary %>
<%#
  description, osver, examples are all appended to become part of summary before
  we move on to actual tags 
-%>
<% if (event.description) { -%>
 * <%- event.description %>
<% } -%>
<% if (event.osver) { -%>
 * <%- event.osver %>
<% } -%>
<% if (event.examples) { -%>
 * <%- event.examples %>
<% } -%>
<% if ('hide' in event && event.hide) { -%>
 * @hide
<% } -%>
<% if (event.deprecated) { -%>
 * <%- event.deprecated %>
<% } -%>
<% if (event.properties) { -%>
<%   event.properties.forEach(function (param) { -%>
<%-    include('param', { param: param }); %>
<%   }); -%>
<% } -%>
<% for (platform in event.since) { -%>
 * @platform <%- platform %> <%- event.since[platform] %>
<% } -%>
 */
