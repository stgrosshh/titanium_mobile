/**
 * @method <%- method.name %>
 * <%- method.summary %>
<%#
   description, osver, examples are all appended to become part of summary before
   we move on to actual tags
-%>
<% if (method.description) { -%>
 * <%- method.description %>
<% } -%>
<% if (method.osver) { -%>
 * <%- method.osver %>
<% } -%>
<% if (method.examples) { %>
 * <%- method.examples %>
<% } -%>
<% if ('hide' in method && method.hide) { -%>
 * @hide
<% } -%>
<% if (method.deprecated) { -%>
 * <%- method.deprecated %>
<% } -%>
<% if (method.parameters) { -%>
<%   method.parameters.forEach(function (param) { -%>
<%- include('param', {param: param}); %>
<%   }); -%>
<% } -%>
<% if (method.returns) { -%>
 * @returns <%- method.returns %>
<% } -%>
<% for (platform in method.since) { -%>
 * @platform <%- platform %> <%- method.since[platform] %>
<% } -%>
 */
